#include <stdio.h>
#include <iostream>
using namespace std;
int main(){
    float a,pi;
    
    pi = 3.14;
    printf("please input a:");
    scanf("%f",&a);
    
    if(a>=0){
        printf(" a >= %f \n",a*a*pi);
        printf(" a >= %f \n",a*2*pi);// area = pi r*2
    }else
     
    cout<<"Hello World";
    return 0;
}
